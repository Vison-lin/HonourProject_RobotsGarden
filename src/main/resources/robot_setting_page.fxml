<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Accordion?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<fx:root minWidth="221.0" prefHeight="532.0" type="javafx.scene.layout.VBox" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
   <Text fx:id="numberRobot" strokeType="OUTSIDE" strokeWidth="0.0" text="NumberRobot ：">
      <VBox.margin>
         <Insets bottom="5.0" left="10.0" right="10.0" top="10.0" />
      </VBox.margin>
      <font>
         <Font name="System Bold" size="15.0" />
      </font></Text>
   <Accordion>
     <panes>
       <TitledPane animated="false" text="List of Algorithm">
         <content>
           <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="170.0" prefWidth="148.0">
                  <children>
                     <ScrollPane layoutX="-2.0" prefHeight="200.0" prefWidth="211.0">
                       <content>
                         <AnchorPane minHeight="0.0" minWidth="0.0" prefWidth="210.0">
                              <children>
                                 <VBox fx:id="listBox"  prefHeight="180.0" prefWidth="200.0">
                                    <children>
                                       <Text fx:id="listAlgorithm" strokeType="OUTSIDE" strokeWidth="0.0" text="ListAlgorithm ：">
                                          <font>
                                             <Font name="System Bold" size="15.0" />
                                          </font>
                                       </Text>
                                    </children>
                                    <padding>
                                       <Insets left="2.0" />
                                    </padding>
                                 </VBox>
                              </children>
                           </AnchorPane>
                       </content>
                     </ScrollPane>
                  </children>
               </AnchorPane>
         </content>
            <font>
               <Font name="System Bold" size="15.0" />
            </font>
       </TitledPane>
     </panes>
      <VBox.margin>
         <Insets left="10.0" right="10.0" />
      </VBox.margin>
   </Accordion>
   <Line endX="120.0" startX="-100.0" stroke="lightgray" strokeLineCap="ROUND" strokeLineJoin="ROUND" strokeWidth="1.0">
      <VBox.margin>
         <Insets left="5.0" right="5.0" top="10.0" />
      </VBox.margin></Line>

    <ComboBox fx:id="overlayRobotSelection" prefWidth="150.0" BorderPane.alignment="CENTER">
      <VBox.margin>
         <Insets bottom="10.0" left="35.0" right="10.0" top="10.0" />
      </VBox.margin></ComboBox>

    <Text fx:id="robotTag" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" textOrigin="CENTER" BorderPane.alignment="CENTER">
      <VBox.margin>
         <Insets bottom="5.0" left="80.0" top="10.0" />
      </VBox.margin></Text>
    <Button fx:id="toggleVision" mnemonicParsing="false" prefWidth="150.0" text="Toggle vision">
      <VBox.margin>
         <Insets bottom="10.0" left="35.0" top="10.0" />
      </VBox.margin></Button>
    <Button fx:id="showStatistic" mnemonicParsing="false" prefWidth="150.0" text="Show Statistic">
      <VBox.margin>
         <Insets bottom="10.0" left="35.0" top="10.0" />
      </VBox.margin></Button>
    <Button fx:id="deleteSelected" mnemonicParsing="false" prefWidth="150.0" text="Delete Selected">
      <VBox.margin>
         <Insets bottom="10.0" left="35.0" top="10.0" />
      </VBox.margin></Button>
    <Button fx:id="detailSetting" mnemonicParsing="false" prefWidth="150.0" text="Setting">
      <VBox.margin>
         <Insets bottom="10.0" left="35.0" top="10.0" />
      </VBox.margin></Button>
</fx:root>
