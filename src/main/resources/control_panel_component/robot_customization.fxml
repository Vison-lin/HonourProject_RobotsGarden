      <Line endX="371.0" fill="#0720ff" startX="-100.0" stroke="#808eff" strokeWidth="3.0">
         <VBox.margin>
            <Insets bottom="5.0" top="10.0" />
         </VBox.margin></Line>
   </children>
</fx:root>
