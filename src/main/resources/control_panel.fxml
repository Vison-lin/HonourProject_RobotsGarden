<?xml version="1.0" encoding="UTF-8"?>

<?import controller.controlpanel.ProgressController?>
<?import controller.controlpanel.RobotGenerationController?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.*?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" prefHeight="500.0" prefWidth="500.0" type="javafx.scene.layout.VBox"
         xmlns="http://javafx.com/javafx/8.0.172-ea">
    <children>
        <Text fx:id="statuscontrolText" strokeType="OUTSIDE" strokeWidth="0.0" text="Status Control"/>

        <ProgressController fx:id="progressController"/>

        <RobotGenerationController fx:id="robotGenerationController"/>

        <Button fx:id="SaveButton" mnemonicParsing="false" text="Save" />
        <Text fx:id="warning" strokeType="OUTSIDE" strokeWidth="0.0" text="Text"/>
        <Text fx:id="mouseCoordinate"/>
        <Text fx:id="statisticDisplay"/>
    </children>
</fx:root>
